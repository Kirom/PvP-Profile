{
  "name": "pvpprofile",
  "version": "1.0.0",
  "description": "World of Warcraft addon for multiple PvP website integration",
  "author": "Kirom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kirom/PvP-Profile.git"
  },
  "keywords": [
    "world-of-warcraft",
    "wow",
    "addon",
    "pvp",
    "check-pvp",
    "drustvar",
    "seramate",
    "armory",
    "arena",
    "classic"
  ],
  "scripts": {
